Host github.com
  User git
  Hostname github.com
  IdentityFile ~/.ssh/id_ecdsa

Host home
  User mute
  Hostname home.mute1008.dev
  Port 1008
  IdentityFile ~/.ssh/id_ecdsa

Host home-over-gateway
  User mute
  Hostname localhost
  Port 2222
  IdentityFile ~/.ssh/id_ecdsa
  ProxyCommand ssh -W %h:%p gateway

Host gateway
  User mute1008
  Hostname 34.85.123.229
  Port 22
  IdentityFile ~/.ssh/id_ecdsa

