export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="af-magic"
plugins=(
  alias-finder
  asdf
  aws
  brew
  command-not-found
  docker
  docker-compose
  extract
  git
  gh
  npm
  ssh
  yarn
  z
)

# alias-finder
zstyle ':omz:plugins:alias-finder' autoload yes

source $ZSH/oh-my-zsh.sh

# User configuration
export LANG=ja_JP.UTF-8
source ~/.dotfiles/source/alias.sh
source ~/.dotfiles/source/export.sh
