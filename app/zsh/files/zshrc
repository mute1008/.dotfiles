export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="af-magic"
plugins=(
  alias-finder
  asdf
  aws
  brew
  command-not-found
  docker
  docker-compose
  extract
  git
  gh
  npm
  ssh
  yarn
  z
)

# alias-finder
zstyle ':omz:plugins:alias-finder' autoload yes

source $ZSH/oh-my-zsh.sh

# User configuration
export LANG=ja_JP.UTF-8
source ~/.dotfiles/source/alias.sh
source ~/.dotfiles/source/export.sh

### MANAGED BY RANCHER DESKTOP START (DO NOT EDIT)
export PATH="/Users/naoya/.rd/bin:$PATH"
### MANAGED BY RANCHER DESKTOP END (DO NOT EDIT)

# The next line updates PATH for the Google Cloud SDK.
if [ -f '$HOME/Documents/tools/google-cloud-sdk/path.zsh.inc' ]; then . '$HOME/Documents/tools/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '$HOME/Documents/tools/google-cloud-sdk/completion.zsh.inc' ]; then . '$HOME/Documents/tools/google-cloud-sdk/completion.zsh.inc'; fi
